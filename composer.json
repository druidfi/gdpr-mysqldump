{
  "name": "druidfi/gdpr-mysqldump",
  "description": "A drop-in replacement for mysqldump that optionally sanitizes DB fields for better GDPR conformity.",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Druid.fi",
      "email": "info@druid.fi"
    },
    {
      "name": "Axel Rutz",
      "email": "axel.rutz@machbarmacher.net"
    },
    {
      "name": "Bomoko",
      "email": "blaize.kaye@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "cweagans/composer-patches": "^1.7",
    "fakerphp/faker": "^1.20",
    "ifsnop/mysqldump-php": "^2.9",
    "matomo/ini": "^3.0",
    "symfony/console": "^4.4.25|^5.0|^6.0",
    "symfony/event-dispatcher": "^4.4.25|^5.0|^6.0",
    "symfony/filesystem": "^5.4 || ^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.15 || ^9"
  },
  "bin": [
    "mysqldump"
  ],
  "autoload": {
    "psr-4": {
      "druidfi\\GdprDump\\": "src/"
    }
  },
  "extra": {
    "patches": {
      "ifsnop/mysqldump-php": {
        "Add getter": "https://patch-diff.githubusercontent.com/raw/ifsnop/mysqldump-php/pull/137.patch"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "cweagans/composer-patches": true
    }
  }
}
